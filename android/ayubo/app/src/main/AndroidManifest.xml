<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="lk.hemas.ayubo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".config.AyuboApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.DashboardActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.DoctorActivity" />
        <activity android:name=".activity.VisitDoctorActivity" />
        <activity
            android:name=".activity.SearchActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".activity.ScheduleActivity"
            android:label="@string/title_activity_schedule"
            android:theme="@style/AppTheme" />
        <activity android:name=".activity.DetailActivity"></activity>
    </application>

</manifest>